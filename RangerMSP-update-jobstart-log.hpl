    <attributes/>
    <GUI>
      <xloc>144</xloc>
      <yloc>208</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Update Logging (workflow start)</name>
    <type>Neo4jCypherOutput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>${graphdb-ds}</connection>
    <cypher>UNWIND $taskparams as t
WITH t,timestamp() as wfstart,trim(replace(t.workflow,'Workflow Name',t.pipeline)) as workflow
MERGE (l:N4jlogging {workflow:workflow})
SET l.lastwfrun=wfstart,l.pipeline=trim(t.pipeline),l.status='running',l.plfolder=t.plfolder,l.plfilename=t.plfilename,l.wffolder=t.wffolder,l.wffilename=t.wffilename
RETURN apoc.date.format(l.lastwfrun,'ms',"yyyy-MM-dd'T'HH:mm:ss",'CST') as wfstart,l.pipeline
;
</cypher>
    <batch_size/>
    <read_only>N</read_only>
    <nr_retries_on_error/>
    <retry>Y</retry>
    <cypher_from_field>N</cypher_from_field>
    <cypher_field/>
    <unwind>Y</unwind>
    <unwind_map>taskparams</unwind_map>
    <returning_graph>N</returning_graph>
    <return_graph_field/>
    <mappings>
      <mapping>
        <parameter>workflow</parameter>
        <field>workflow</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>pipeline</parameter>
        <field>pipeline</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>plfolder</parameter>
        <field>plfolder</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>wffolder</parameter>
        <field>wffolder</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>wffilename</parameter>
        <field>wffilename</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>plfilename</parameter>
        <field>plfilename</field>
        <type>String</type>
      </mapping>
    </mappings>
    <returns>
      <return>
        <name>jobstart</name>
        <type>String</type>
        <source_type>String</source_type>
      </return>
      <return>
        <name>l.jobname</name>
        <type>String</type>
        <source_type>String</source_type>
      </return>
      <return>
        <name>l.transname</name>
        <type>String</type>
        <source_type>String</source_type>
      </return>
    </returns>
    <attributes/>
    <GUI>
      <xloc>400</xloc>
      <yloc>208</yloc>
    </GUI>
  </transform>
  <transform_error_handling>
  </transform_error_handling>
  <attributes/>
</pipeline>
