      </mapping>
    </mappings>
    <returns>
      <return>
        <name>l.jobname</name>
        <type>String</type>
        <source_type>String</source_type>
      </return>
      <return>
        <name>l.transname</name>
        <type>String</type>
        <source_type>String</source_type>
      </return>
    </returns>
    <attributes/>
    <cluster_schema/>
    <GUI>
      <xloc>368</xloc>
      <yloc>208</yloc>
      <draw>Y</draw>
    </GUI>
